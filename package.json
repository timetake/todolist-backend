{
  "name": "ts-express",
  "version": "0.0.0",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start:prod": "node -r module-alias/register ./dist --env=production",
    "start": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@routes": "dist/routes",
    "@schema": "dist/schema",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.4",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "^3.6.11",
    "@types/jsonfile": "^6.0.0",
    "@types/mongoose": "^5.10.5",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "jasmine": "^3.7.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
